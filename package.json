{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "description": "## Project setup ``` npm install ```",
  "author": "",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "deploy": "aws s3 sync ./dist s3://web.mypasel.com",
    "test:unit": "jest",
    "test:unit:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --no-cache --runInBand"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "babel-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js, vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ]
  },
  "main": "babel.config.js",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.5",
    "js-cookie": "^2.2.0",
    "sails.io.js": "^1.2.1",
    "socket.io-client": "^2.2.0",
    "v-offline": "^1.1.0",
    "vee-validate": "^2.2.9",
    "vue": "^2.6.10",
    "vue-content-loader": "^0.2.2",
    "vue-cookie": "^1.1.4",
    "vue-ctk-date-time-picker": "^2.1.1",
    "vue-drawer-layout": "^1.3.0",
    "vue-loading-overlay": "^3.2.0",
    "vue-phone-number-input": "^0.2.0",
    "vue-promise-btn": "^2.0.2",
    "vue-router": "^3.0.3",
    "vue-star-rating": "^1.6.1",
    "vue-tour": "^1.1.0",
    "vue2-google-maps": "^0.10.6",
    "vuex": "^3.1.1",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/eslint-config-prettier": "^4.0.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^24.8.0",
    "jest-serializer-vue": "^2.0.2",
    "jest-transform-stub": "^2.0.0",
    "vue-jest": "^3.0.4",
    "vue-template-compiler": "^2.6.10",
    "webpack-bundle-analyzer": "^3.3.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/airbnb",
      "@vue/prettier"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "bugs": {
    "url": "https://github.com/bluedistro/mypasel-web-gh/issues"
  },
  "homepage": "https://github.com/bluedistro/mypasel-web-gh#readme",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bluedistro/mypasel-web-gh.git"
  }
}
